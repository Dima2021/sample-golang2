{
  "settingsInheritedFrom": "Dima2021/whitesource-config@master",
  "remediateSettings": {
    "workflowRules": {
      "enabled": false
    },
    "enableRenovate": true,
    "extends": [
      "config:base",
      "github>whitesource/merge-confidence:beta"
    ],
    "packageRules": [
      {
        "matchDatasources": ["golang-version"],
        "enabled": true
      },
      {
        "matchManagers": ["gomod"],
        "enabled": true,
        "registryUrls": ["https://proxy.golang.org"]
      }
    ],
    "customEnvVariables": {
      "GOPROXY": "https://proxy.golang.org,direct"
    },
   "hostRules": [
      {
        "matchHost": "https://proxy.golang.org",
        "username": "admin",
        "password": "admin"
      }
    ]
  }
}
